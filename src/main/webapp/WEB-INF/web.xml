    </servlet>
    <servlet-mapping>
        <servlet-name>ProductSelectServlet</servlet-name>
        <url-pattern>/ProductSelectServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddAmountServlet</servlet-name>
        <servlet-class>AddAmountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAmountServlet</servlet-name>
        <url-pattern>/AddAmountServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>DeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/DeleteServlet</url-pattern>
    </servlet-mapping>
</web-app>
